{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "strict": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts",
    "**/*.vue",
    "types/**/*.d.ts"
  ],
  "exclude": [
    "node_modules",
    ".nuxt/dist",
    ".output",
    "dist"
  ]
}
